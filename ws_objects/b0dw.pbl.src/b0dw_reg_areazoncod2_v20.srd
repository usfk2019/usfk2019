 retrieve="SELECT A.SVCCOD
     , A.NODENO
     , A.AREACOD
     , B.AREANM
     , B.AREAGROUP
     , B.COUNTRYCOD
     , A.ZONCOD
     , A.CRT_USER
     , A.UPDT_USER
     , A.CRTDT
     , A.UPDTDT
     , A.PGM_ID
	  , A.ZONCOD_COST
     , A.PRICEPLAN
  FROM AREZONCOD2 A
     , AREAMST    B
