 retrieve="SELECT A.CUSTOMERID
     , B.CUSTOMERNM
     , A.CONTRACTSEQ
     , A.PRICEPLAN
     , A.STATUS
     , A.ACTIVEDT
     , A.TERMDT
     , A.BIL_TODT
     , C.ONEOFFCHARGE_YN
     , C.ITEMCOD
     , A.DIRECT_PAYTYPE
     , A.ENDDT
  FROM CONTRACTMST A
     , CUSTOMERM   B
     , ( SELECT AA.CONTRACTSEQ
              , BB.ONEOFFCHARGE_YN
              , BB.ITEMCOD
           FROM CONTRACTDET AA
              , ITEMMST     BB
          WHERE AA.ITEMCOD   = BB.ITEMCOD
            AND BB.PREBIL_YN = 'Y'         
                 ) C
     , SVCMST      D
 WHERE A.CUSTOMERID  = B.CUSTOMERID
   AND A.CONTRACTSEQ = C.CONTRACTSEQ
   AND A.SVCCOD      = D.SVCCOD" )
