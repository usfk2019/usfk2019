 retrieve="SELECT  con.contractseq, 
 		con.customerid,
 		cus.customernm,
		con.status,
        con.svccod,
		con.priceplan, 
		sus.fromdt, 
		sus.todt,
		con.contractno,
        con.prmtype, 
		con.partner,
		con.reg_partner, 
		con.sale_partner, 
		con.bil_fromdt,
		sus.seq
    FROM customerm cus, contractmst con, suspendinfo sus
   WHERE cus.customerid = con.customerid 
   	AND con.contractseq = sus.contractseq
ORDER BY con.contractseq desc, cus.customerid" )
