 retrieve="  SELECT A.VALIDKEY
       , A.FROMDT
       , A.TODT
       , A.SVCCOD
       , A.CUSTOMERID
       , A.VPASSWORD
       , A.USE_YN
       , A.CRT_USER
       , A.UPDT_USER
       , A.CRTDT
       , A.UPDTDT
       , A.PGM_ID
       , A.STATUS
       , A.PRICEPLAN
       , A.CONTRACTSEQ
       , A.GKID
       , A.SVCTYPE
       , A.ORDERNO
       , A.PID
       , A.auth_method
       , A.validitem1
       , A.validitem2
       , A.validitem3
		 , A.validkey_loc
       , B.langtypeNM
    FROM VALIDINFO A
       , LANGTYPEINFO B 
   WHERE A.CUSTOMERID = :as_customerid
     AND A.SVCCOD     = B.SVCCOD(+)
     AND A.LANGTYPE   = B.LANGTYPE(+)
ORDER BY A.VALIDKEY ASC
       , A.FROMDT ASC
       , A.SVCCOD ASC  " arguments=(("as_customerid", string)) )
