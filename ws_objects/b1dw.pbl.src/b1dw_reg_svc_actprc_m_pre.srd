 retrieve="  SELECT ~"SVCORDER~".~"ORDERNO~",   
         ~"SVCORDER~".~"ORDERDT~",   
         ~"SVCORDER~".~"CUSTOMERID~",   
         ~"CUSTOMERM~".~"CUSTOMERNM~",   
         ~"SVCORDER~".~"REQUESTDT~",   
         ~"SVCORDER~".~"SVCCOD~",   
         ~"SVCORDER~".~"CRT_USER~",   
         ~"SVCORDER~".~"PRICEPLAN~",   
         ~"SVCORDER~".~"PARTNER~",   
         ~"SVCORDER~".~"PRMTYPE~",   
         ~"SVCORDER~".~"REG_PARTNER~",   
         ~"SVCORDER~".~"MAINTAIN_PARTNER~",   
         ~"SVCORDER~".~"SALE_PARTNER~",   
         ~"SVCORDER~".~"SETTLE_PARTNER~"  
    FROM ~"CUSTOMERM~",   
         ~"SVCORDER~"  
   WHERE ( ~"CUSTOMERM~".~"CUSTOMERID~" = ~"SVCORDER~".~"CUSTOMERID~" )   
     AND svcorder.svccod in ( select svccod from svcmst
                      where svctype = ( select rtrim(ltrim(ref_content)) 
										from sysctl1t where module = 'B0' and ref_no = 'P101') )
ORDER BY ~"SVCORDER~".~"ORDERNO~" DESC   
" )
