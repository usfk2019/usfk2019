         con.priceplan, 
         con.prmtype, 
         con.reg_partner,
         con.maintain_partner,
		   con.sale_partner,
         con.settle_partner,
         to_char(sus.fromdt,'yyyymmdd') fromdt,
         to_char(sus.todt,'yyyymmdd') todt,
		   to_date(null) actdt,
         con.partner partner, 
		 'N' act_gu,
         to_char(null) partnername,
         to_char(null) remark,
			con.suspend_type
    FROM customerm cus, contractmst con, suspendinfo sus
   WHERE cus.customerid = con.customerid
