       cmt.svccod, cmt.priceplan, cmt.prmtype, cmt.status, cmt.activedt,
       cmt.bil_fromdt, cmt.reg_partner, cmt.maintain_partner, cmt.sale_partner,
       cmt.settle_partner, cus.payid, cus.payid, svc.requestdt, svc.termtype,
       svc.partner, svc.orderno, to_date(null) termdt, to_date(null) enddt, svc.status,
	   cmt.pricemodel, cmt.first_sale_amt, cmt.balance, svcmst.svctype, svc.remark, cus1.customernm
  FROM svcorder svc, contractmst cmt, customerm cus, svcmst, customerm cus1
 WHERE svc.ref_contractseq = cmt.contractseq 
   and cus.customerid = cmt.customerid
   AND cmt.customerid = cus.customerid
   and cmt.svccod = svcmst.svccod(+)
   and cus.payid = cus1.customerid
" )
