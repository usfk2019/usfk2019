 retrieve="SELECT distinct A.CUSTOMERID
     , B.CUSTOMERNM
     , A.CONTRACTSEQ
     , A.PRICEPLAN
     , A.STATUS
     , A.pricemodel 
     , A.ACTIVEDT
     , A.ENDDT
     , A.TERMDT
     , A.BIL_FROMDT
     , A.BIL_TODT     
     , A.DIRECT_PAYTYPE
     , A.PAYDAYS
	  , A.PAYDAYS_TYPE
	  , A.SVCCOD
  FROM CONTRACTMST A
     , CUSTOMERM   B
     , ( SELECT AA.CONTRACTSEQ
              , BB.ONEOFFCHARGE_YN
              , BB.ITEMCOD
              , BB.PREBIL_YN
           FROM CONTRACTDET AA
              , ITEMMST     BB
          WHERE AA.ITEMCOD   = BB.ITEMCOD
            AND BB.PREBIL_YN = 'Y'         
                 ) C
 WHERE A.CUSTOMERID  = B.CUSTOMERID
   AND A.CONTRACTSEQ = C.CONTRACTSEQ"  sort="customerid A contractseq D " )
