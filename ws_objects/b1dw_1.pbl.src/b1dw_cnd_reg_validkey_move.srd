 retrieve="  SELECT A.PARTNER
       , B.PRICEPLAN
       , B.VALIDKEY_TYPE
       , COUNT(B.VALIDKEY)                  TOT_QTY
       , SUM(DECODE(B.SALE_FLAG,'0',1,0))   STOCK_QTY
       , SUM(DECODE(B.SALE_FLAG,'1',1,0))   SALE_QTY
    FROM PARTNERMST  A
       , VALIDKEYMST B
   WHERE A.PARTNER   = B.PARTNER
     AND A.PREFIXNO  = B.PARTNER_PREFIX  
GROUP BY A.PARTNER
       , B.PRICEPLAN
       , B.VALIDKEY_TYPE
" )
