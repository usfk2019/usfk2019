 retrieve="  SELECT ~"CARDTEXT~".~"SENDDT~" ,
         SUM(DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",1,1) , 'A' , DECODE(~"CARDTEXT~".~"STATUS~",'AA',1,0) , 'C' , 1 , 'I' , 1 , 0)) AUTH_SUCCESS ,  
         SUM(DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",1,1) , 'A' , DECODE(~"CARDTEXT~".~"STATUS~",'AA',~"CARDTEXT~".~"AMT~",0) , 'C' , ~"CARDTEXT~".~"AMT~" , 'I' , ~"CARDTEXT~".~"AMT~" , 0)) AUTH_SUCCESS_AMT ,  
         SUM(DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",1,1) , 'A' , 1 , 'B' , 1 , 'C' , 1 , 'I' , 1 , 0)) AUTH_REQUIRE , 
         SUM(DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",1,1) , 'A' , ~"CARDTEXT~".~"AMT~" , 'B' , ~"CARDTEXT~".~"AMT~" , 'C' , ~"CARDTEXT~".~"AMT~" , 'I' , ~"CARDTEXT~".~"AMT~" , 0)) AUTH_REQUIRE_AMT , 
         SUM(DECODE(~"CARDTEXT~".~"STATUS~" , 'BB' , 1 , 0)) AUTH_CANCEL , 
         SUM(DECODE(~"CARDTEXT~".~"STATUS~" , 'BB' , ~"CARDTEXT~".~"AMT~" , 0)) AUTH_CANCEL_AMT , 
         SUM(DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",1,1) , 'C' , DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",2,1),'1',1,'2',1,'3',1,'4',1,'5',1,'6',1,'7',1,0) , 0)) CAP_SUCCESS ,  
			SUM(DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",1,1) , 'C' , DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",2,1),'1',~"CARDTEXT~".~"AMT~",'2',~"CARDTEXT~".~"AMT~",'3',~"CARDTEXT~".~"AMT~",'4',~"CARDTEXT~".~"AMT~",'5',~"CARDTEXT~".~"AMT~",'6',~"CARDTEXT~".~"AMT~",'7',~"CARDTEXT~".~"AMT~",0) , 0)) CAP_SUCCESS_AMT ,  
         SUM(DECODE(~"CARDTEXT~".~"INSTATUS~" , 'S' , 1 , 0)) OUTAMT , 
         SUM(DECODE(~"CARDTEXT~".~"INSTATUS~" , 'S' , ~"CARDTEXT~".~"AMT~" , 0)) OUTAMT_AMT,
         SUM(decode(cardtext.worktype, 'B', DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",1,1) , 'A' , DECODE(~"CARDTEXT~".~"STATUS~",'AA',1,0) , 'C' , 1 , 'I' , 1 , 0))) suc_a ,  
         SUM(decode(cardtext.worktype, 'B', DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",1,1) , 'C' , DECODE(SUBSTR(~"CARDTEXT~".~"STATUS~",2,1),'1',1,'2',1,'3',1,'4',1,'5',1,'6',1,'7',1,0) , 0))) suc_c
    FROM ~"CARDTEXT~"  
GROUP BY ~"CARDTEXT~".~"SENDDT~"  
ORDER BY ~"CARDTEXT~".~"SENDDT~" DESC   
" )
