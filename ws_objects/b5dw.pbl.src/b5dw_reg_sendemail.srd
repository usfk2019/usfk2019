 column=(type=char(50) updatewhereclause=yes name=bil_email dbname="reqinfo.bil_email" dbalias="req.bil_email" )
 column=(type=datetime updatewhereclause=yes name=trdt dbname="reqamtinfo.trdt" dbalias="inv.trdt" )
 column=(type=char(1) updatewhereclause=yes name=send_check dbname="send_check" values="	Y/	N" )
 column=(type=char(10) updatewhereclause=yes name=billinginfo_pay_method dbname="billinginfo.pay_method" dbalias="bil.pay_method" )
 retrieve="SELECT cus.customerid, 
       cus.customernm, 
       req.bil_email, 
       inv.trdt, 
       'Y' send_check,
       bil.pay_method
	FROM customerm cus, reqinfo req, reqamtinfo inv, billinginfo bil 
  WHERE req.payid 	= cus.customerid 
    AND inv.payid	= req.payid 
    AND inv.trdt 	= req.trdt
    AND cus.customerid =  bil.customerid
    AND req.inv_method = :as_inv_method
 ORDER BY req.payid
" arguments=(("as_inv_method", string)) )
