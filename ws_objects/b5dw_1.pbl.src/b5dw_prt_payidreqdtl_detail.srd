 retrieve="SELECT Z.PAYID
     , Z.CUSTOMERNM
	  , Z.PAY_METHOD
     , Z.REQNUM
     , MIN(Z.SEQ) SEQ
     , Z.TRCOD
     , Z.TRCODNM
     , SUM(Z.TRAMT) TRAMT
     , Z.TRDT
     , Z.PAYDT
     , Z.CRTDT
     , Z.REMARK
  FROM (
SELECT A.PAYID
     , B.CUSTOMERNM
	  , B.PAY_METHOD
     , A.REQNUM
     , A.SEQ
     , A.TRCOD
     , G.TRCODNM
     , A.TRAMT
     , A.TRDT
     , A.PAYDT
     , A.CRTDT
     , A.REMARK
  FROM REQDTL A
     , REQINFO B
     , TRCODE G
 WHERE A.PAYID    = B.PAYID
   AND A.TRDT     = B.TRDT
   AND A.TRCOD    = G.TRCOD
   AND B.CHARGEDT = :as_chargedt
   AND TO_CHAR(A.TRDT,'YYYYMMDD') = :as_trdt
   AND B.CURRENCY_TYPE = :as_currency
   AND A.MARK IS NULL
UNION ALL
SELECT C.PAYID
     , D.CUSTOMERNM
	  , D.PAY_METHOD
     , C.REQNUM
     , C.SEQ
     , C.TRCOD
     , H.TRCODNM
     , C.TRAMT
     , C.TRDT
     , C.PAYDT
     , C.CRTDT
     , C.REMARK
  FROM REQDTL C
     , REQINFOH D
     , TRCODE H
 WHERE C.PAYID    = D.PAYID
   AND C.TRDT     = D.TRDT
   AND C.TRCOD    = H.TRCOD
   AND D.CHARGEDT = :as_chargedt
   AND TO_CHAR(C.TRDT,'YYYYMMDD') = :as_trdt
   AND D.CURRENCY_TYPE = :as_currency
   AND C.MARK IS NULL
UNION ALL  
SELECT E.PAYID
     , F.CUSTOMERNM
	  , F.PAY_METHOD
     , E.REQNUM
     , E.SEQ
     , E.TRCOD
     , I.TRCODNM
     , E.TRAMT
     , E.TRDT
     , E.PAYDT
     , E.CRTDT
     , E.REMARK
  FROM REQDTLH E
     , REQINFOH F
     , TRCODE I
 WHERE E.PAYID    = F.PAYID
   AND E.TRDT     = F.TRDT
   AND E.TRCOD    = I.TRCOD
   AND F.CHARGEDT = :as_chargedt
   AND TO_CHAR(E.TRDT,'YYYYMMDD') = :as_trdt
   AND F.CURRENCY_TYPE = :as_currency
   AND E.MARK IS NULL ) Z
 WHERE Z.TRAMT <> 0
 GROUP BY Z.PAYID
     , Z.CUSTOMERNM
	  , Z.PAY_METHOD
     , Z.REQNUM
	  , Z.SEQ
     , Z.TRCOD
     , Z.TRCODNM
     , Z.TRDT
     , Z.PAYDT
     , Z.CRTDT
     , Z.REMARK
     " arguments=(("as_chargedt", string),("as_trdt", string),("as_currency", string)) )
