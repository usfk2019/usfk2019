        cus.customernm,
		  cus.status,
		  bil.pay_method,
		  cus.location,
		  cus.buildingno,
		  cus.roomno,
		  cus.phone1,
		  cus.phone2,
        DLYMST.AMOUNT,
        bil.overdue_MONTHS,
        cus.contact
  FROM DLYMST,
       billinginfo bil,
       customerm cus
