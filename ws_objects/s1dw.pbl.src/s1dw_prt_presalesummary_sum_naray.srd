 retrieve="  SELECT YYYYMM,             
         SUM(FACEFIRSTSUM_AMT) FACEFIRSTSUM_AMT,   /*액면가기초누적액*/   
         SUM(SALEFIRSTSUM_AMT) SALEFIRSTSUM_AMT,   /*판매가기초누적액*/   
         SUM(USEDFIRSTSUM_AMT) USEDFIRSTSUM_AMT,   /*사용액기초누적액*/   
         SUM(FACEFIRST_AMT)    FACEFIRST_AMT,      /*기초액면가*/   
         SUM(SALEFIRST_AMT)    SALEFIRST_AMT,      /*기초판매가*/   
         SUM(SALEFIRST_AMT*decode(FACEFIRST_AMT,0,0,(decode (sign(USEDFIRSTSUM_AMT - FACEFIRST_AMT),1,FACEFIRST_AMT,USEDFIRSTSUM_AMT))/FACEFIRST_AMT)+ salefirstsum_amt *decode(facefirstsum_amt ,0,0,(decode(sign(USEDFIRSTSUM_AMT - FACEFIRST_AMT),1,USEDFIRSTSUM_AMT - FACEFIRST_AMT,0))/ facefirstsum_amt) ) as sales
    FROM ~"PRE_SALESSUMMARY~" 
   WHERE (FACEFIRSTSUM_AMT > 0 or FACEFIRST_AMT > 0)
GROUP BY YYYYMM" )
