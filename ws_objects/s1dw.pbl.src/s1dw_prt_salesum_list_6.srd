sum(decode(to_char(sum.sale_month, 'mm'), '02', nvl(sum.saleamt, 0), 0))/:as_rate sale02,
sum(decode(to_char(sum.sale_month, 'mm'), '03', nvl(sum.saleamt, 0), 0))/:as_rate sale03,
sum(decode(to_char(sum.sale_month, 'mm'), '04', nvl(sum.saleamt, 0), 0))/:as_rate sale04,
sum(decode(to_char(sum.sale_month, 'mm'), '05', nvl(sum.saleamt, 0), 0))/:as_rate sale05,
sum(decode(to_char(sum.sale_month, 'mm'), '06', nvl(sum.saleamt, 0), 0))/:as_rate sale06,
sum(decode(to_char(sum.sale_month, 'mm'), '07', nvl(sum.saleamt, 0), 0))/:as_rate sale07,
sum(decode(to_char(sum.sale_month, 'mm'), '08', nvl(sum.saleamt, 0), 0))/:as_rate sale08,
sum(decode(to_char(sum.sale_month, 'mm'), '09', nvl(sum.saleamt, 0), 0))/:as_rate sale09,
sum(decode(to_char(sum.sale_month, 'mm'), '10', nvl(sum.saleamt, 0), 0))/:as_rate sale10,
sum(decode(to_char(sum.sale_month, 'mm'), '11', nvl(sum.saleamt, 0), 0))/:as_rate sale11,
sum(decode(to_char(sum.sale_month, 'mm'), '12', nvl(sum.saleamt, 0), 0))/:as_rate sale12,
nvl(sum(sum.saleamt), 0) / :as_rate sale_total,
0  rate01, 0 rate02, 0 rate03, 0 rate04, 0 rate05, 0 rate06, 0 rate07, 0 rate08, 0 rate09,
0 rate10, 0 rate11, 0 rate12
FROM itemsalesummary sum
GROUP BY sum.currency_type, sum.ctype1
