 retrieve="SELECT Z.CATEGORY
     , '1건수' A
     , '2시간' B
     , '3금액' C
     , Z.BILCNT
     , Z.BILTIME
     , Z.BILCOST
  FROM ( SELECT '1시간대' CATEGORY
              , SUM(NVL(BILCNT1,0)) BILCNT
              , round(SUM(NVL(BILTIME1,0))/60,0) BILTIME
              , round(SUM(NVL(BILCOST1,0))/:ac_rate,3) BILCOST
           FROM CDRSUMMARY
          WHERE WORKDT BETWEEN :as_frdt AND :as_todt
			   AND CURRENCY_TYPE = :as_currency
		  UNION ALL
		   SELECT '2시간대' CATEGORY
              , SUM(NVL(BILCNT2,0)) BILCNT
              , round(SUM(NVL(BILTIME2,0))/60,0) BILTIME
              , round(SUM(NVL(BILCOST2,0))/:ac_rate,3) BILCOST
           FROM CDRSUMMARY
          WHERE WORKDT BETWEEN :as_frdt AND :as_todt
			   AND CURRENCY_TYPE = :as_currency
		  UNION ALL
		   SELECT '3시간대' CATEGORY
              , SUM(NVL(BILCNT3,0)) BILCNT
              , round(SUM(NVL(BILTIME3,0))/60,0) BILTIME
              , round(SUM(NVL(BILCOST3,0))/:ac_rate,3) BILCOST
           FROM CDRSUMMARY
          WHERE WORKDT BETWEEN :as_frdt AND :as_todt
			   AND CURRENCY_TYPE = :as_currency
		  UNION ALL
		   SELECT '4시간대' CATEGORY
              , SUM(NVL(BILCNT4,0)) BILCNT
              , round(SUM(NVL(BILTIME4,0))/60,0) BILTIME
              , round(SUM(NVL(BILCOST4,0))/:ac_rate,3) BILCOST
           FROM CDRSUMMARY
          WHERE WORKDT BETWEEN :as_frdt AND :as_todt
			   AND CURRENCY_TYPE = :as_currency ) Z" arguments=(("ac_rate", number),("as_frdt", string),("as_todt", string),("as_currency", string)) )
