 retrieve="SELECT priceplan, areagroup,
             SUM(decode(daynum,1,bilcnt1+bilcnt2+bilcnt3+bilcnt4,0)) bilcnt0, 
       ROUND(SUM(decode(daynum,1,biltime1+biltime2+biltime3+biltime4,0))/60,0) biltime0, 
       ROUND(SUM(decode(daynum,1,bilcost1+bilcost2+bilcost3+bilcost4,0))/:ac_rate,3) bilcost0,
             SUM(decode(daynum,2,bilcnt1+bilcnt2+bilcnt3+bilcnt4,0)) bilcnt1, 
       ROUND(SUM(decode(daynum,2,biltime1+biltime2+biltime3+biltime4,0))/60,0) biltime1, 
       ROUND(SUM(decode(daynum,2,bilcost1+bilcost2+bilcost3+bilcost4,0))/:ac_rate,3) bilcost1,
             SUM(decode(daynum,3,bilcnt1+bilcnt2+bilcnt3+bilcnt4,0)) bilcnt2, 
       ROUND(SUM(decode(daynum,3,biltime1+biltime2+biltime3+biltime4,0))/60,0) biltime2, 
       ROUND(SUM(decode(daynum,3,bilcost1+bilcost2+bilcost3+bilcost4,0))/:ac_rate,3) bilcost2,
             SUM(decode(daynum,4,bilcnt1+bilcnt2+bilcnt3+bilcnt4,0)) bilcnt3, 
       ROUND(SUM(decode(daynum,4,biltime1+biltime2+biltime3+biltime4,0))/60,0) biltime3, 
       ROUND(SUM(decode(daynum,4,bilcost1+bilcost2+bilcost3+bilcost4,0))/:ac_rate,3) bilcost3,
             SUM(decode(daynum,5,bilcnt1+bilcnt2+bilcnt3+bilcnt4,0)) bilcnt4, 
       ROUND(SUM(decode(daynum,5,biltime1+biltime2+biltime3+biltime4,0))/60,0) biltime4, 
       ROUND(SUM(decode(daynum,5,bilcost1+bilcost2+bilcost3+bilcost4,0))/:ac_rate,3) bilcost4,
             SUM(decode(daynum,6,bilcnt1+bilcnt2+bilcnt3+bilcnt4,0)) bilcnt5, 
       ROUND(SUM(decode(daynum,6,biltime1+biltime2+biltime3+biltime4,0))/60,0) biltime5, 
       ROUND(SUM(decode(daynum,6,bilcost1+bilcost2+bilcost3+bilcost4,0))/:ac_rate,3) bilcost5,
             SUM(decode(daynum,7,bilcnt1+bilcnt2+bilcnt3+bilcnt4,0)) bilcnt6, 
       ROUND(SUM(decode(daynum,7,biltime1+biltime2+biltime3+biltime4,0))/60,0) biltime6, 
       ROUND(SUM(decode(daynum,7,bilcost1+bilcost2+bilcost3+bilcost4,0))/:ac_rate,3) bilcost6
  FROM CDRSUMMARY2
 GROUP BY priceplan, areagroup" arguments=(("ac_rate", number))  sort="areagroup A " )
