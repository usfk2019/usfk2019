	    DECODE(DECODE((TRUNC(sum(biltime)/60,0)*60),0,'',to_char(TRUNC(sum(biltime)/60,0)) || '분')|| to_char(sum(biltime)-(TRUNC(sum(biltime)/60,0)*60),'00')||'초','분초','0초',DECODE((TRUNC(sum(biltime)/60,0)*60),0,'',to_char(TRUNC(sum(biltime)/60,0)) || '분')|| to_char(sum(biltime)-(TRUNC(sum(biltime)/60,0)*60),'00')||'초') calltime,
	    DECODE(DECODE((TRUNC(sum(systime)/60,0)*60),0,'',to_char(TRUNC(sum(systime)/60,0)) || '분')|| to_char(sum(systime)-(TRUNC(sum(systime)/60,0)*60),'00')||'초','분초','0초',DECODE((TRUNC(sum(systime)/60,0)*60),0,'',to_char(TRUNC(sum(systime)/60,0)) || '분')|| to_char(sum(systime)-(TRUNC(sum(systime)/60,0)*60),'00')||'초') s_time,
		 (Sum(biltime) / DECODE(Sum(totcnt-failcnt),0,100000,Sum(totcnt-failcnt))) mid_sec,
