 retrieve="SELECT   A.SHOPID,   
         A.CUSTOMERID,   
         A.PAYID,   
         A.ITEMCOD,   
         c.ITEMnm,   
         A.REGCOD,  
			b.keynum, 		
         b.facnum, 
         A.PAYAMT payamt,   
         A.PAYCNT paycnt,
         A.DCTYPE dctype,
         A.TAXAMT,
         decode(c.surtaxyn,'N','*',' ') as surtaxyn
    FROM DAILYPAYMENT A, regcodmst b, itemmst c
   WHERE a.regcod 		= b.regcod (+)
     and a.itemcod 		= c.itemcod (+)
  	  and A.APPROVALNO 	= :as_arg
" arguments=(("as_arg", string)) )
