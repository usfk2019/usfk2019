 retrieve="SELECT A.CUSTOMERID,
		 B.CUSTOMERNM,
		 A.ITEMCOD,
       C.ITEMNM,
		 A.REGCOD,
		 A.PAYMETHOD,
		 A.OPERATOR, 
       B.MEMBERID,
       D.regtype,
a.shopid,
		 A.PAYCNT,
		 A.PAYAMT,
a.paydt,
a.crtdt,
a.approvalno,
a.pgm_id,
a.manual_yn
FROM DAILYPAYMENT A, CUSTOMERM B, itemmst c, regcodmst d
WHERE A.CUSTOMERID = B.CUSTOMERID
  AND A.ITEMCOD 	 = C.ITEMCOD
  and a.regcod 	 = d.regcod

union all

SELECT A.CUSTOMERID,
		 B.CUSTOMERNM,
		 A.ITEMCOD,
       C.ITEMNM,
		 A.REGCOD,
		 A.PAYMETHOD,
		 A.OPERATOR, 
       B.MEMBERID,
       D.regtype,
a.shopid,
		 A.PAYCNT,
		 A.PAYAMT,
a.paydt,
a.crtdt,
a.approvalno,
a.pgm_id,
a.manual_yn
FROM DAILYPAYMENTh A, CUSTOMERM B, itemmst c, regcodmst d
WHERE A.CUSTOMERID = B.CUSTOMERID
  AND A.ITEMCOD 	 = C.ITEMCOD
  and a.regcod 	 = d.regcod" )
