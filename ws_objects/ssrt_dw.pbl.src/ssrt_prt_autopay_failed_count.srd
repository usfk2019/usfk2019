 retrieve="SELECT A.GROUPID, 
       C.BASENM,
		 NVL(A.Status,'ERR') STATUS,
       SUM(A.REQAMT) REQAMT,
		 COUNT(*) CNT
 FROM KCPBATCH A, CUSTOMERM B,  BASEMST C
 WHERE (A.PAYID   = B.CUSTOMERID) 
   AND (A.GROUPID = C.BASECOD )
   AND ( A.Result = 'N' )
GROUP BY A.GROUPID, 
       C.BASENM,
		 A.Status
ORDER BY A.GROUPID, A.Status 

"  sort="groupid A status A " )
