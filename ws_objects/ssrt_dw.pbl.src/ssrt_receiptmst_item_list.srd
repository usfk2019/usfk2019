 retrieve="  SELECT A.CUSTOMERID,   
         A.ITEMCOD,   
         B.ITEMNM,   
         A.REGCOD,   
         A.BASECOD,   
         A.PAYID,   
         A.TRDT,   
         A.PAYDT,
         A.PAYCNT,   
         A.PAYAMT,
         A.DCTYPE,
         B.TRCOD,
         DECODE(B.SURTAXYN, 'N', '*', ' ') SURTAXYN,
         NVL(A.TAXAMT, 0)  TAXAMT
    FROM DAILYPAYMENT A, itemmst B 
   WHERE APPROVALNO = :as_arg 
     AND A.ITEMCOD 	= B.ITEMCOD 
" arguments=(("as_arg", string)) )
