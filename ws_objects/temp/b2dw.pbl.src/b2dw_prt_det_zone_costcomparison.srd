 retrieve="select a.zoncod
     , b.zonnm
     , round(sum(nvl(a.retail_time,0) + nvl(a.wholesale_time,0)) / 60 , 2) USE_TIME
     , round(sum(nvl(a.cost_time,0)) / 60 , 2) COST_TIME
     , sum(nvl(a.retail_amt,0))
     , sum(nvl(a.wholesale_amt,0))
     , sum(nvl(a.cost,0))
  from zone_cost_detail a, zone b
 where to_char(a.costdt,'yyyymmdd') >= :as_fromdt
   and to_char(a.costdt,'yyyymmdd') <= :as_todt
   and a.zoncod = b.zoncod
GROUP BY a.zoncod, b.zonnm" arguments=(("as_fromdt", string),("as_todt", string)) )
