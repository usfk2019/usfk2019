 retrieve="SELECT carr.areacod,
       area.codenm areagroup,
       carr.cdr_type,
	   zon.codenm,   
	   sum(carr.BILCNT)  ctuse,   
	   sum(carr.BILTIME) tmuse,
	   decode(:as_flag,'D',trunc(sum(nvl(carr.biltime,0))/60,0),'O',round(sum(nvl(carr.biltime,0))/60,0),ceil(sum(nvl(carr.biltime,0))/60)) tmuse_min,
	   sum(carr.BILAMT)  amtuse 
 FROM CARRIER_AMOUNT carr, 
      (select code, codenm from syscod2t where grcode = 'C130' and use_yn = 'Y') zon,
      (select code, codenm from syscod2t where grcode = 'C140' and use_yn = 'Y') area
 WHERE carr.zoncod = zon.code(+) and carr.AREAGROUP = area.code(+)
GROUP BY area.codenm, carr.cdr_type, carr.areacod ,zon.codenm" arguments=(("as_flag", string))  sort="areagroup A cdr_type A areacod A codenm A " )
