         ~"P_REFILLLOG~".~"REFILL_TYPE~",   
         ~"PARTNERMST~".~"PARTNER~",   
         ~"P_REFILLLOG~".~"REFILL_AMT~",   
         ~"P_REFILLLOG~".~"SALE_AMT~",   
         ~"P_CARDMST~".~"BALANCE~",   
         ~"P_REFILLLOG~".~"EDAY~",   
         ~"P_CARDMST~".~"PRICEPLAN~",   
         ~"P_REFILLLOG~".~"REMARK~",
         ~"P_REFILLLOG~".~"REFILL_PLACE~",
         ~"P_REFILLLOG~".~"PAYTYPE~"  
    FROM ~"P_CARDMST~",   
         ~"P_REFILLLOG~",   
         ~"PARTNERMST~"  
   WHERE ( ~"P_CARDMST~".~"PARTNER_PREFIX~" = ~"PARTNERMST~".~"PREFIXNO~" ) and  
         ( ~"P_CARDMST~".~"PID~" = ~"P_REFILLLOG~".~"PID~" )   
