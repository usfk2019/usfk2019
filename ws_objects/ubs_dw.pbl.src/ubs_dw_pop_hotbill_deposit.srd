 retrieve="SELECT B.PAYSEQ,
       B.ITEMCOD, 
	    A.ITEMNM, 
		 B.PAYDT,   
		 B.SHOPID,   
		 B.OPERATOR,   
		 B.PAYMETHOD,   
		 B.REGCOD,   
		 B.PAYAMT,   
		 B.PAYCNT,   
		 B.REMARK,
       B.REMARK2,
		 B.APPROVALNO,
		 C.GUBUN
FROM   ITEMMST A, DAILYPAYMENT B, ( SELECT 'I' AS GUBUN, IN_ITEM AS ITEMCOD
												FROM   DEPOSIT_REFUND
												UNION ALL
												SELECT 'O', OUT_ITEM
												FROM   DEPOSIT_REFUND ) C
WHERE  A.ITEMCOD = B.ITEMCOD
AND    B.PAYID = :as_payid
AND    B.ITEMCOD = C.ITEMCOD
UNION ALL
SELECT B.PAYSEQ,
       B.ITEMCOD, 
	    A.ITEMNM, 
		 B.PAYDT,   
		 B.SHOPID,   
		 B.OPERATOR,   
		 B.PAYMETHOD,   
		 B.REGCOD,   
		 B.PAYAMT,   
		 B.PAYCNT,   
		 B.REMARK,
       B.REMARK2,
		 B.APPROVALNO,
		 C.GUBUN
FROM   ITEMMST A, DAILYPAYMENTH B, ( SELECT 'I' AS GUBUN, IN_ITEM AS ITEMCOD
												 FROM   DEPOSIT_REFUND
												 UNION ALL
												 SELECT 'O', OUT_ITEM
												 FROM   DEPOSIT_REFUND ) C
WHERE  A.ITEMCOD = B.ITEMCOD
AND    B.PAYID = :as_payid
AND    B.ITEMCOD = C.ITEMCOD
" arguments=(("as_payid", string))  sort="dailypayment_payseq D gubun A " )
