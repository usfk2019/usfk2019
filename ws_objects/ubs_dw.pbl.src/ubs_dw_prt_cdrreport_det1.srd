 retrieve="SELECT  X.CUSTOMERID, Z.CUSTOMERNM, Z.BUILDINGNO, Z.ROOMNO, X.CONTRACTSEQ, X.PRICEPLAN, 
        X.VALIDKEY, X.ITEMCOD, NVL(Y.DC_RANGE, 0) DC_RANGE,
        X.BILTIME, X.BILAMT, X.DCBILAMT, X.DCBIL_RANGE, X.BILTIME_RANGE,
        DECODE(NVL(Y.DC_RANGE, 0), 0, 'X', (SELECT NVL(MAX(W.ITEMCOD), 'X') FROM LIMIT_CONTROL_ZONE W
                                    		  WHERE  W.PRICEPLAN = X.PRICEPLAN
                                    		  AND    W.TARGET_CODE = X.ITEMCOD )) AS CONT_ITEM   
FROM (  SELECT  A.CUSTOMERID, A.CONTRACTSEQ, A.PRICEPLAN, A.VALIDKEY, A.ITEMCOD,
                A.BILTIME, A.BILAMT, A.DCBILAMT, A.DCBIL_RANGE, A.BILTIME_RANGE,
                NVL(B.ITEMCOD, 'X') AS CONT_ITEM
        FROM  ( SELECT CUSTOMERID, CONTRACTSEQ, PRICEPLAN, VALIDKEY, ITEMCOD,
                       SUM(BILTIME) AS BILTIME, SUM(BILAMT) AS BILAMT, SUM(DCBILAMT) AS DCBILAMT,
                       SUM(NVL(ADDAMT, 0)) AS DCBIL_RANGE, SUM(NVL(BILTIME_RANGE, 1)) AS BILTIME_RANGE
                FROM   POST_BILCDR
                WHERE  PAYID = :arg_payid
                AND    CONTRACTSEQ = :arg_contractseq
                AND    TRDT = :arg_trdt
                GROUP BY CUSTOMERID, CONTRACTSEQ, PRICEPLAN, VALIDKEY, ITEMCOD, CONNECTFEE ) A, 
		        ( SELECT C.PRICEPLAN, C.ITEMCOD, C.TARGET_CODE
                FROM   CONTRACTMST A, CONTRACTDET B, LIMIT_CONTROL_ZONE C
                WHERE  A.CONTRACTSEQ = :arg_contractseq
                AND    A.CONTRACTSEQ = B.CONTRACTSEQ
                AND    B.BIL_FROMDT <= ADD_MONTHS(:arg_trdt, -1)
                AND  ( B.BIL_TODT IS NULL OR B.BIL_TODT >= ADD_MONTHS(:arg_trdt, -1) )
                AND    A.PRICEPLAN = C.PRICEPLAN
                AND    B.ITEMCOD = C.ITEMCOD ) B 
        WHERE   A.PRICEPLAN = B.PRICEPLAN(+)
        AND     A.ITEMCOD   = B.TARGET_CODE(+) ) X,
     (  SELECT  X.ITEMCOD, NVL(Y.BASICRANGE, 0) AS DC_RANGE
        FROM (  SELECT  X.PRICEPLAN, X.ITEMCOD, MAX(Y.FROMDT) FROMDT
                FROM  ( SELECT A.PRICEPLAN, B.ITEMCOD, NVL(B.BIL_CURFROMDT, B.BIL_FROMDT) AS USED_FROMDT
                        FROM   CONTRACTMST A, CONTRACTDET B
                        WHERE  A.CONTRACTSEQ = :arg_contractseq
                        AND    A.CONTRACTSEQ = B.CONTRACTSEQ ) X, PRICEPLAN_RATE2 Y
                WHERE   X.PRICEPLAN = Y.PRICEPLAN
                AND     X.ITEMCOD   = Y.ITEMCOD 
                AND     Y.FROMDT <= X.USED_FROMDT
                GROUP BY X.PRICEPLAN, X.ITEMCOD ) X, PRICEPLAN_RATE2 Y
        WHERE   X.PRICEPLAN = Y.PRICEPLAN
        AND     X.ITEMCOD   = Y.ITEMCOD
        AND     X.FROMDT    = Y.FROMDT ) Y, CUSTOMERM Z
WHERE  X.CONT_ITEM = Y.ITEMCOD(+)
AND    X.CUSTOMERID = Z.CUSTOMERID
ORDER BY DC_RANGE DESC, X.ITEMCOD ASC" arguments=(("arg_payid", string),("arg_contractseq", number),("arg_trdt", date)) )
