 retrieve=" SELECT ADM.EQUIPSEQ, 
        ADM.ADTYPE, 
       ADM.MODELNO, 
        ADM.SERIALNO, 
       ADM.STATUS, 
       ADM.VALID_STATUS,
        ADM.ADSTAT,
        ADM.MAKERCD, 
        ADM.USE_YN,
       TO_CHAR(ADM.IDATE, 'mm-dd-yyyy') IDATE, 
        ADM.ISEQNO,
       ADM.ENTSTORE, 
        ADM.IDAMT, 
        TO_CHAR(ADM.RETDT, 'mm-dd-yyyy') RETDT,
       ADM.MV_PARTNER, 
        TO_CHAR(ADM.SNMOVEDT, 'mm-dd-yyyy') MOVEDT, 
        ADM.PID, 
        ADM.SN_PARTNER, 
       ADM.SALE_FLAG, 
        TO_CHAR(ADM.SALEDT, 'mm-dd-yyyy') SALEDT, 
        ADM.SALE_AMT,
        ADM.CONTRACTSEQ, 
        ADM.ORDERNO,
       ADM.DLVSTAT, 
        TO_CHAR(ADM.DLVDT, 'mm-dd-yyyy') DLVDT,
        ADM.UPDTDT,
        ADM.REMARK, 
        ADM.UPDT_USER,
        ADM.SHIPNO, 
        ADM.SPEC_ITEM1,
        ADM.CONTNO,
      ADM.TO_PARTNER,
        ADM.STATUS AS STATUS_OLD
     FROM EQUIPMST ADM
WHERE 1=1
  AND NVL(ADM.CONTRACTSEQ, ADM.ORDERNO) = :as_orderno
  AND ADM.CUSTOMERID = :as_customerid" update="EQUIPMST" updatewhere=0 updatekeyinplace=no arguments=(("as_orderno", string),("as_customerid", string)) )
