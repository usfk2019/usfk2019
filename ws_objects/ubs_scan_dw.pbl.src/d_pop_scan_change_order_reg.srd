 column=(type=char(10) updatewhereclause=yes name=order_type dbname="svcorder.order_type" )
 column=(type=char(10) updatewhereclause=yes name=priceplan dbname="svcorder.priceplan" )
 retrieve="SELECT   0 AS CHK,
         C.ORDERNO,
         C.ORDERDT,
         C.ORDER_TYPE,
         C.PRICEPLAN
  FROM   SVCORDER C,
         (SELECT   A.PRICEPLAN,
                   A.ORDER_TYPE,
                   A.DOC_TYPE,
                   A.FROMDT
            FROM   PRICEPLAN_DOC_TYPE A,
                   (  SELECT   A.PRICEPLAN,
                               A.ORDER_TYPE,
                               MAX (A.FROMDT) AS FROMDT
                        FROM   PRICEPLAN_DOC_TYPE A
                       WHERE   a.fromdt <= SYSDATE
                    GROUP BY   A.PRICEPLAN, A.ORDER_TYPE) B
           WHERE       A.PRICEPLAN = B.PRICEPLAN
                   AND A.ORDER_TYPE = B.ORDER_TYPE
                   AND A.FROMDT = B.FROMDT) x
 WHERE       C.PRICEPLAN = X.PRICEPLAN
         AND C.ORDER_TYPE = X.ORDER_TYPE
         AND C.ORDERNO NOT IN (SELECT   ORDERNO FROM ORDER_DOCGROUP)
         AND X.DOC_TYPE LIKE :as_doc_type
         AND X.FROMDT <= C.ORDERDT
         AND C.CUSTOMERID = :as_customerid" update="SVCORDER" updatewhere=0 updatekeyinplace=no arguments=(("as_customerid", string),("as_doc_type", string))  sort="orderno A " )
