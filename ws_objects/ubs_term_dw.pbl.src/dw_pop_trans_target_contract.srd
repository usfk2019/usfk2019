 retrieve="SELECT B.CUSTOMERID, 
            B.PAYID, 
            B.FIRSTNAME ||' ' || B.MIDNAME ||' ' || B.LASTNAME AS CUSTOMERNM,
            B.HOTBILLFLAG,
            DECODE(B.HOTBILLFLAG,'S', '고객_전체핫빌', 'E','고객_전체핫빌완료', 'H', '고객_부분핫빌', B.HOTBILLFLAG)||'('||B.HOTBILLFLAG||')' AS HOTBILLFLAGNM,
            A.CONTRACTSEQ ,
            A.BILL_HOTBILLFLAG,
            DECODE(A.BILL_HOTBILLFLAG,'R', '계약_핫빌선택', 'P','계약_핫빌처리중', 'H', '계약_부분핫빌', 'S','계약_전체핫빌',A.BILL_HOTBILLFLAG)||'('|| A.BILL_HOTBILLFLAG||')'  AS BILL_HOTBILLFLAGNM,
            (SELECT S.SVCDESC FROM SVCMST S WHERE S.SVCCOD = A.SVCCOD) AS SVCDESC, 
            P.PRICEPLAN_DESC ,
            TO_CHAR( A.BIL_FROMDT, 'YYYY-MM-DD') AS BIL_FROMDT ,
            TO_CHAR( A.BIL_TODT, 'YYYY-MM-DD') AS BIL_TODT ,
            (SELECT SC.CODENM FROM SYSCOD2T SC WHERE SC.GRCODE ='B300' AND SC.CODE =A.STATUS) AS STATUS ,
            (SELECT SC.CODENM FROM SYSCOD2T SC WHERE SC.GRCODE ='B306' AND SC.CODE =A.TERMTYPE) AS TERMTYPE,
            (SELECT P.PARTNERNM FROM PARTNERMST P WHERE P.PARTNER = A.PARTNER) AS ACT_PARTNER        
FROM SAMS.CONTRACTMST A ,SAMS.CUSTOMERM B , SAMS.PRICEPLANMST P, TERMCUST.TRANS_TARGET T
WHERE A.CUSTOMERID = B.CUSTOMERID
    AND A.PRICEPLAN = P.PRICEPLAN
    AND B.CUSTOMERID = T.CUSTOMERID
    AND T.WORKNO = :as_workno
" arguments=(("as_workno", string))  sort="customerid A payid A contractseq A bil_fromdt A " )
